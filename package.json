{
  "name": "crag-track",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "dump-demo": "sqlite3 ./demo-db/sqlite.db .dump > ./src/lib/db/data/demo.sql",
    "studio": "drizzle-kit studio --config drizzle.config.ts",
    "generate": "drizzle-kit generate --config drizzle.config.ts",
    "migrate": "drizzle-kit push --config drizzle.config.ts"
  },
  "dependencies": {
    "@auth/sveltekit": "^0.14.1",
    "@floating-ui/dom": "^1.6.3",
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@skeletonlabs/skeleton": "2.9.0",
    "@skeletonlabs/tw-plugin": "0.3.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "better-sqlite3": "^9.6.0",
    "luxon": "^3.4.4",
    "ol": "^9.1.0",
    "proj4": "^2.11.0",
    "remark-html": "^16.0.1",
    "remark-parse": "^11.0.0",
    "svelte-pdf": "^1.0.20",
    "tailwindcss": "3.4.1",
    "unified": "^11.0.4",
    "vega": "^5.28.0",
    "vega-embed": "^6.24.0",
    "vega-lite": "^5.17.0",
    "webdav": "^5.5.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/adapter-node": "^5.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/eslint": "^8.56.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.11.30",
    "@types/proj4": "^2.5.5",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "autoprefixer": "10.4.19",
    "drizzle-kit": "^0.22.7",
    "drizzle-orm": "^0.31.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "^0.3.3",
    "vitest": "^1.6.0"
  },
  "type": "module"
}
